<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="333.0" prefWidth="435.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafx.StartController">
    <padding>
        <Insets bottom="100.0" left="100.0" right="100.0" top="100.0" />
    </padding>
   <children>
      <Label />
      <Label fx:id="helloLabel" alignment="CENTER" contentDisplay="CENTER" text="じゃんけんjava" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="23.0" />
         </font>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </Label>
      <Button fx:id="NextButton" mnemonicParsing="false" onAction="#onNextButtonAction" text="start !">
         <padding>
            <Insets bottom="20.0" left="40.0" right="40.0" top="20.0" />
         </padding></Button>
      <Button fx:id="ReturnButton" mnemonicParsing="false" onAction="#onReturnButtonAction" text="戻る" />
      <Button fx:id="CloseButton" mnemonicParsing="false" onAction="#onCloseButtonAction" text="閉じる" />
   </children>
</VBox>
